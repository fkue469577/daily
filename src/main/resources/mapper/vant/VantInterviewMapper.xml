<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.bc.finance.modular.vant.mapper.VantInterviewMapper">

    <select id="articles" resultType="com.bc.finance.modular.vant.entity.VantInterview">
        select ID, type, title, create_time
        from vant_interview
        where `ids` like concat('%', #{channelId}, '%')
        and `type`='DOC'

    </select>
</mapper>
